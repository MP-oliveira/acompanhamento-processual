{
  "version": 2,
  "buildCommand": "echo 'No build needed for Node.js'",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "vercel-server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel-server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
